<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jeremy_minie.cloakme" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.jeremy_minie.cloakme.HelloAgainCrmApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.jeremy_minie.cloakme.MainActivity"
            android:label="@string/title_activity_main"
            android:theme="@style/AppTheme.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.jeremy_minie.cloakme.user.UserActivity"
            android:label="test"
            android:parentActivityName="com.jeremy_minie.cloakme.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.jeremy_minie.cloakme.MainActivity" />
        </activity>
    </application>

</manifest>
